{
  "name": "multitrack_rock",
  "input_dirs": [
    ".\\data\\midi\\clean_midi\\"
  ],
  "output_path": "data/datasets/multitrack_rock.h5",
  "genre_tsv_path": ".\\data\\midi\\clean_midi\\genre.tsv",
  "allowed_genres": [
    "Alternative rock"
  ],
  "min_tracks": 2,
  "max_tracks": 64,
  "min_notes_per_track": 20,
  "min_duration_seconds": 30.0,
  "max_duration_seconds": 180.0,
  "resolution": 24,
  "time_signature": [
    4,
    4
  ],
  "quantize": true,
  "quantize_grid": 1,
  "remove_empty_tracks": true,
  "segment_length": 1024,
  "max_padding_ratio": 0.3,
  "enable_transposition": false,
  "transposition_semitones": [
    -5,
    -4,
    -3,
    -2,
    -1,
    1,
    2,
    3,
    4,
    5,
    6
  ],
  "enable_tempo_variation": false,
  "tempo_variation_range": [
    0.9,
    1.1
  ],
  "tempo_variation_steps": 3,
  "encoder_type": "multitrack",
  "max_seq_length": 1024,
  "encode_velocity": true,
  "positions_per_bar": 32,
  "max_samples": 10000,
  "random_seed": null,
  "verbose": true
}