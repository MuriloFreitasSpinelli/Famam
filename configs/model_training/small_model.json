{
  "model_name": "small_model",
  "num_pitches": 128,
  "max_time_steps": 500,
  "num_instruments": 129,
  "lstm_units": [64],
  "dense_units": [32],
  "dropout_rate": 0.2,
  "recurrent_dropout": 0.1,
  "bidirectional": false,
  "return_sequences": true,
  "instrument_embedding_dim": 8,
  "genre_embedding_dim": 8,
  "batch_size": 16,
  "epochs": 50,
  "shuffle": true,
  "optimizer": "adam",
  "learning_rate": 0.001,
  "loss_function": "mse",
  "metrics": ["mae"],
  "beta_1": 0.9,
  "beta_2": 0.999,
  "epsilon": 1e-7,
  "momentum": 0.0,
  "nesterov": false,
  "rho": 0.9,
  "lr_schedule": "constant",
  "lr_decay_rate": 0.96,
  "lr_decay_steps": 10000,
  "lr_drop_factor": 0.5,
  "lr_drop_epochs": 10,
  "lr_alpha": 0.0,
  "lr_power": 1.0,
  "lr_end_learning_rate": 0.0001,
  "lr_patience": 5,
  "lr_min_delta": 0.0001,
  "lr_cooldown": 0,
  "lr_min_lr": 1e-7,
  "l1_reg": 0.0,
  "l2_reg": 0.0,
  "kernel_constraint_max_value": null,
  "recurrent_constraint_max_value": null,
  "use_gradient_clipping": false,
  "gradient_clip_value": 1.0,
  "mixed_precision": false,
  "use_early_stopping": true,
  "early_stopping_patience": 5,
  "early_stopping_min_delta": 0.0001,
  "early_stopping_monitor": "val_loss",
  "early_stopping_mode": "min",
  "restore_best_weights": true,
  "use_checkpointing": true,
  "checkpoint_monitor": "val_loss",
  "checkpoint_mode": "min",
  "save_best_only": true,
  "save_weights_only": false,
  "use_tensorboard": true,
  "tensorboard_log_dir": "./logs",
  "tensorboard_histogram_freq": 1,
  "tensorboard_write_graph": true,
  "tensorboard_update_freq": "epoch",
  "output_dir": "./models",
  "save_history": true,
  "save_final_model": true,
  "random_seed": 42
}
